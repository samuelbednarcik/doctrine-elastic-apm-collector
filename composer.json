{
    "name": "samuelbednarcik/doctrine-elastic-apm-collector",
    "description": "Elastic APM span collector for Doctrine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Bednarcik",
            "email": "bednarciks@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "samuelbednarcik/elastic-apm-agent": "^0.2.1",
        "doctrine/dbal": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "SamuelBednarcik\\ElasticAPMAgent\\Collectors\\Doctrine\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5"
    },
    "scripts": {
        "phpstan": "phpstan analyse src --level 7"
    }
}
